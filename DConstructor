class DConstructor {
	int x;
	int y;
	DConstructor(){
		x= 10;
		y = 20;
	}
	void show(){
		System.out.println(x);
		System.out.println(y);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		DConstructor dc = new DConstructor();
		dc.show();
		DConstructor dc1 = new DConstructor();
		dc1.show();
	}
}
