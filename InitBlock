class InitBlock {
	int x;{
	this.x = 10;
	System.out.println("Init Block");
	}
	InitBlock(){
		System.out.println("Default");
		System.out.println(x);
	}
	InitBlock(int y){
		System.out.println(x+y);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new InitBlock();
	}

}
