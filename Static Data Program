class Hello{
	 String name;
	 int salary;
	 void get(String s1,int s2){
		 name = s1;
		 salary = s2;
	 }
	 void show(){
		 System.out.println(name);
		 System.out.println(salary);
	 }
	 public static void main(String args[]){
		 Hello h1 = new Hello();
		 h1.get("Rajnish",101);
		 h1.show();
		 Hello ss1 = new Hello();
		 ss1.get("Vijay",102);
		 ss1.show();
	 }
}
