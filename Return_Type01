class Return_Type01{
	void cc(int a, int b){
		int sum = 0;
		sum = (a+b)/10;
		System.out.println("Int method Total sum = "+sum);
		
	}
	double dd(double d, double dd){
		double sum = 0;
		sum = (d+dd)*5.0;
		return sum;
	}
	public static void main(String []args){
		new Return_Type01().cc(10,15);
		System.out.println("Double method Total sum = "+ new Return_Type01().dd(11.2,10.0));
	}
}
